{
  "name": "hasura-example-app",
  "description": "Example of a webapp of Hasura using React and Redux with Babel and Webpack.",
  "author": "<AuthorName> <<AuthorEmail>> (<AuthorSite>)",
  "license": "MIT",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://<git-site>/<author/<repo>"
  },
  "homepage": "",
  "keywords": [
    "hasura",
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start-prod": "better-npm-run start-prod",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/dev.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "NAMESPACE": "api",
        "BASE_DOMAIN": "stackexchange.com",
        "SCHEME": "http"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "history": "1.17.0",
    "import": "0.0.6",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.7",
    "react-helmet": "^2.2.0",
    "react-progress-bar-plus": "^1.2.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-bootstrap": "^0.20.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.3.2",
    "redux-thunk": "^1.0.3",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "^0.0.8",
    "bootstrap-loader": "^1.0.9",
    "bootstrap-sass": "^3.3.5",
    "clean-webpack-plugin": "^0.1.6",
    "css-loader": "^0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-config-xo-space": "^0.8.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.6",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^3.2.3",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
